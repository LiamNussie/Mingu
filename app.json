{
  "expo": {
    "name": "Mingu",
    "slug": "Mingu",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/Mingu.png",
    "userInterfaceStyle": "light",
    "jsEngine": "hermes",
    "splash": {
      "image": "./assets/Mingu-white.png",
      "resizeMode": "contain",
      "backgroundColor": "#FF6B6B",
      "hideExpoText": true
    },
    "ios": {
      "supportsTablet": true,
      "jsEngine": "hermes"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/Mingu.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.blondev.Mingu",
      "jsEngine": "hermes",
      "proguardMap": true,
      "enableProguardInReleaseBuilds": true,
      "enableHermes": true,
      "enableShrinkResourcesInReleaseBuilds": true,
      "minifyEnabled": true
    },
    "web": {
      "favicon": "./assets/Mingu.png"
    },
    "extra": {
      "eas": {
        "projectId": "082b79e7-9a6d-45e2-8ef4-00df1362fe83"
      }
    },
    "plugins": []
  }
